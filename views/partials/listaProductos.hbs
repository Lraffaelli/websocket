<table class="table table-dark table-striped ">
    <thead>
        <tr>
            <th>Id</th>
            <th>Producto</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>

        <tbody>
         {{#each productos}}
             <tr>
                 <th>{{id}}</th>
                 <th>{{title}}</th>
                 <th>{{price}}</th>
                 <th><img class="img-thumbnail" src={{thumbnail}} alt={{id}}></th>
             </tr>
         {{/each}}
        </tbody>
    </thead>

</table>

